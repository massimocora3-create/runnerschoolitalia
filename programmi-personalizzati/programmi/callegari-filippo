<script>
  // --- CONFIG ---
  // Usiamo un hash univoco per questa pagina per la chiave di localStorage
  const PAGE_HASH = "c16f5b918e77fcf75b5cf004635559976751c11b169e13ce0215b68aa3f2dbf7";
  const STORAGE_KEY = "rs_program_unlock_" + PAGE_HASH;

  // Password in chiaro per questa pagina
  const key = "CalFil.2026";

  const lock = document.getElementById("lock");
  const app = document.getElementById("app");
  const unlockBtn = document.getElementById("unlock");
  const pwInput = document.getElementById("pw");
  const lockMsg = document.getElementById("lockMsg");

  // Mostra contenuto e nasconde il lock
  function showApp() {
    lock.style.display = "none";
    lock.setAttribute("aria-hidden", "true");
    app.style.display = "block";
    lockMsg.style.display = "none";
    pwInput.value = "";
  }

  // Controllo iniziale: se giÃ  sbloccato, niente password
  let alreadyUnlocked = false;
  try {
    alreadyUnlocked = (localStorage.getItem(STORAGE_KEY) === "ok");
  } catch (e) {
    alreadyUnlocked = false;
  }

  if (alreadyUnlocked) {
    showApp();
  } else {
    lock.style.display = "block";
    lock.setAttribute("aria-hidden", "false");
    app.style.display = "none";
  }

  // Verifica password
  function tryUnlock() {
    const inputPassword = pwInput.value.trim();

    if (inputPassword === key) {
      try {
        localStorage.setItem(STORAGE_KEY, "ok");
      } catch (e) {}

      showApp(); // âœ… nessun redirect, resti sulla stessa pagina
    } else {
      lockMsg.style.display = "block";
    }
  }

  // Eventi sui controlli
  unlockBtn.addEventListener("click", function(e){
    e.preventDefault();
    tryUnlock();
  });

  pwInput.addEventListener("keydown", function(e){
    if(e.key === "Enter") tryUnlock();
  });
</script>

